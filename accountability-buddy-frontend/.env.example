# Base URL for the API (e.g., https://example.com/api)
REACT_APP_API_BASE_URL=

# Default language for the application (e.g., "en")
REACT_APP_DEFAULT_LANGUAGE=

# Default theme mode (e.g., "light" or "dark")
REACT_APP_THEME_MODE=

# Google Analytics ID (e.g., "UA-XXXXXXXXX-X")
REACT_APP_GOOGLE_ANALYTICS_ID=

# Sentry DSN for error tracking (optional)
REACT_APP_SENTRY_DSN=

# Enable analytics (true or false)
REACT_APP_ENABLE_ANALYTICS=

# Stripe Public Key (for payments integration)
REACT_APP_STRIPE_PUBLIC_KEY=

# Application Name (e.g., "My App")
REACT_APP_NAME=

# Application Version (e.g., "1.0.0")
REACT_APP_VERSION=
